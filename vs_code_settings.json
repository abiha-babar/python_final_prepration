{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "One Dark Pro Flat",
    // "tabnine.experimentalAutoImports": true,
    "http.proxy": "",
    "http.proxyAuthorization": null,
    "http.proxyStrictSSL": false,
    "python.formatting.provider": "black",
    "prettier.singleQuote": true,
    "prettier.semi": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.tabWidth": 4,
    "editor.formatOnSave": true,
    "python.languageServer": "Pylance",
    "[python]": {
        "editor.formatOnSave": true
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vs-kubernetes": {
        "vscode-kubernetes.kubectl-path.linux": "/home/dev/.local/state/vs-kubernetes/tools/kubectl/kubectl",
        "vscode-kubernetes.minikube-path.linux": "/home/dev/.local/state/vs-kubernetes/tools/minikube/linux-amd64/minikube",
        "vscode-kubernetes.helm-path.linux": "/home/dev/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm"
    },
    "files.autoSave": "afterDelay",
    "typescript.updateImportsOnFileMove.enabled": "always"

}
